{
  "name": "react-playground",
  "description": "Playground environment for GraphQL, React, and more",
  "version": "1.0.0",
  "homepage": "https://github.com/twolfson/react-playground",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/react-playground.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/react-playground/issues"
  },
  "main": "server/index",
  "engines": {
    "node": ">= 4.0.0"
  },
  "license": "Unlicense",
  "scripts": {
    "precheck": "eslint *.js server/ test/",
    "lint": "eslint *.js server/ test/ --max-warnings 0",
    "start": "bin/react-playground-server",
    "start-develop": "ENV=development nodemon -x bin/react-playground-server",
    "test": "npm run precheck && npm run test-server && npm run lint",
    "test-server": "ENV=test mocha --reporter dot test/server/"
  },
  "dependencies": {
    "express": "~4.15.3",
    "methods": "~1.1.2",
    "shallow-settings": "~0.1.0",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "async": "~2.4.1",
    "chai": "~4.0.0",
    "cheerio": "~1.0.0-rc.1",
    "eslint": "~3.19.0",
    "eslint-config-twolfson": "git://github.com/twolfson/eslint-config-twolfson#d181e30",
    "foundry": "~4.3.2",
    "foundry-release-git": "~2.0.2",
    "mocha": "~3.1.2",
    "nodemon": "~1.11.0",
    "request": "~2.81.0"
  },
  "keywords": [],
  "foundry": {
    "releaseCommands": [
      "foundry-release-git"
    ]
  },
  "private": true
}
