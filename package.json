{
  "name": "react-playground",
  "description": "Playground environment for GraphQL, React, and more",
  "version": "1.0.0",
  "homepage": "https://github.com/twolfson/react-playground",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/react-playground.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/react-playground/issues"
  },
  "main": "server/index",
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "Unlicense",
  "scripts": {
    "build": "ENV=production webpack",
    "develop": "ENV=development webpack --watch",
    "preinstall": "if test \"$(basename \"$_\")\" != \"yarn\"; then echo \"Expected \\`yarn\\` to be run for scripts but got \\`$_\\`\" 1>&2; exit 1; fi",
    "postinstall": "yarn run build",
    "precheck": "eslint --ext .js --ext .jsx *.js bin/ browser/ config/ server/ test/",
    "lint": "eslint --ext .js --ext .jsx *.js bin/ browser/ config/ server/ test/ --max-warnings 0",
    "start": "babel-node bin/react-playground-server",
    "start-develop": "ENV=development nodemon --ignore browser --ignore browser-dist -x 'babel-node bin/react-playground-server'",
    "test": "npm run precheck && npm run build && npm run test-server && npm run test-browser-single && npm run lint",
    "test-browser-single": "ENV=test karma start --single-run",
    "test-browser-continuous": "ENV=test karma start",
    "test-browser-debug": "ENV=test karma start --browsers Chrome",
    "test-server": "ENV=test mocha --compilers js:babel-register --reporter dot --recursive test/server/"
  },
  "dependencies": {
    "babel-loader": "~7.0.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "body-parser": "~1.17.2",
    "express": "~4.15.3",
    "express-graphql": "~0.6.6",
    "express-react-views": "~0.10.2",
    "express-session": "~1.15.3",
    "express-session-level": "~1.0.0",
    "graphql": "~0.10.1",
    "http-errors": "~1.6.1",
    "leveldown": "~1.7.1",
    "levelup": "~1.3.8",
    "methods": "~1.1.2",
    "prop-types": "~15.5.10",
    "react": "~15.5.4",
    "react-dom": "~15.5.4",
    "shallow-settings": "~0.1.0",
    "underscore": "~1.8.3",
    "uuid": "~3.0.1"
  },
  "devDependencies": {
    "async": "~2.4.1",
    "babel-cli": "~6.24.1",
    "babel-eslint": "~7.2.3",
    "babel-preset-env": "~1.5.1",
    "babel-register": "~6.24.1",
    "chai": "~4.0.0",
    "cheerio": "~1.0.0-rc.1",
    "enzyme": "~2.8.2",
    "eslint": "~3.19.0",
    "eslint-config-twolfson": "git://github.com/twolfson/eslint-config-twolfson#072a0a6",
    "eslint-plugin-react": "~7.0.1",
    "foundry": "~4.3.2",
    "foundry-release-git": "~2.0.2",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-mocha": "~1.3.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-webpack": "~2.0.3",
    "mocha": "~3.1.2",
    "nodemon": "~1.11.0",
    "react-addons-test-utils": "~15.5.1",
    "react-test-renderer": "~15.5.4",
    "request": "~2.81.0",
    "sinon": "~2.3.4",
    "webpack": "~2.6.1",
    "webpack-livereload-plugin": "~0.11.0"
  },
  "keywords": [],
  "foundry": {
    "releaseCommands": [
      "foundry-release-git"
    ]
  },
  "private": true
}
