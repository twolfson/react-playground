{
  "env": {
    "development": {
      "presets": [
        ["env", {"modules": false}],
        "react"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-class-properties"
      ]
    },
    "production": {
      "presets": [
        "env",
        "react"
      ],
      "plugins": [
        "transform-class-properties"
      ]
    }
  }
}
